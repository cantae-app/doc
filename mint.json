{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Music Player",
  "logo": {
    "dark": "/assets/svg/dark.svg",
    "light": "/assets/svg/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#e11d48",
    "light": "#be123c",
    "dark": "#e11d48",
    "anchors": {
      "from": "#e11d48",
      "to": "#be123c"
    }
  },
  "topbarLinks": [],
  "tabs": [],
  "anchors": [
    {
      "name": "Documentation",
      "icon": "lines-leaning",
      "url": "documentation"
    },
    {
      "name": "Support",
      "icon": "question",
      "url": "support"
    },
    {
      "name": "Download",
      "icon": "angles-down",
      "url": "https://cantae.app"
    },
    {
      "name": "Changelog",
      "icon": "shapes",
      "url": "help"
    }
  ],
  "topAnchor": {
    "icon": "house",
    "name": "Home"
  },
  "primaryTab": {
    "name": "Home"
  },
  "navigation": [
    {
      "group": "Getting started",
      "pages": [
        "documentation/getting-started/introduction",
        "documentation/getting-started/quick-start",
        "documentation/getting-started/interface",
        {
          "group": "Pages",
          "pages": [
            "documentation/getting-started/pages/song",
            "documentation/getting-started/pages/album",
            "documentation/getting-started/pages/artist",
            "documentation/getting-started/pages/favorite",
            "documentation/getting-started/pages/playlist",
            "documentation/getting-started/pages/search",
            "documentation/getting-started/pages/queue",
            "documentation/getting-started/pages/settings",
            "documentation/getting-started/pages/lyrics"
          ]
        }
      ]
    },
    {
      "group": "Help",
      "pages": [
        "help/changelog",
        "help/troubleshooting"
      ]
    },
    {
      "group": "Getting started",
      "pages": [
        "support/getting-started/introduction",
        "support/getting-started/faq"
      ]
    },
    {
      "group": "Resources",
      "pages": [
        "support/license",
        "support/privacy",
        "support/terms"
      ]
    }
  ],
  "feedback": {
    "raiseIssue": true,
    "suggestEdit": true,
    "thumbsRating": true
  },
  "redirects": [
    {
      "source": "/source/path",
      "destination": "/destination/path"
    }
  ]
}